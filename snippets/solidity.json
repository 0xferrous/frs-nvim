{
  "import from": {
    "prefix": "imf",
    "body": "import {${1:imports}} from \"${2:path}\""
  },
  "fn": {
    "prefix": "fn",
    "body": "function ${1:name}(${2:args}) ${3|external,public,internal|} {$0}"
  },
  "fn view": {
    "prefix": "fnv",
    "body": "function ${1:name}(${2:args}) ${3|external,public,internal|} ${4|view,pure|} returns (${5:returns}) {$0}"
  },
  "fn returns": {
    "prefix": "fnr",
    "body": "function ${1:name}(${2:args}) ${3|external,public,internal|} returns {$0}"
  }
}
